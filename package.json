{
    "name": "@jdultra/threedtiles",
    "version": "11.1.13",
    "description": "An OGC 3DTiles viewer for Three.js",
    "main": "dist/threedtiles.min.js",
    "types": "dist/entry.d.ts",
    "files": [
        "dist/**/*",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build:types": "tsc --declaration --emitDeclarationOnly --allowJs --outDir dist",
        "build": "webpack --config webpack.prod.config.js && npm run build:types",
        "build-dev": "webpack --config webpack.dev.config.js",
        "watch": "webpack --watch --config webpack.dev.config.js",
        "dev": "webpack-dev-server --config webpack.dev.config.js",
        "generate-docs": "jsdoc --configure jsdoc.json --verbose"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ebeaufay/threedtiles"
    },
    "keywords": [
        "OGC",
        "3D",
        "3DTiles",
        "Tiles",
        "Three.js",
        "Three"
    ],
    "author": "Emeric Beaufays",
    "license": "MIT",
    "peerDependencies": {
        "three": "0.170.0"
        
    },
    "dependencies": {
        "js-utils-z": "^1.2.1",
        "path-browserify": "^1.0.1",
        "proj4": "^2.10.0",
        "regenerator-runtime": "^0.13.11",
        "uuid": "^8.3.2",
        "meshoptimizer": "^0.20.0"
    },
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-syntax-import-meta": "^7.10.4",
        "@babel/preset-env": "^7.21.4",
        "@types/three": "^0.167.1",
        "babel-loader": "^9.1.2",
        "copy-webpack-plugin": "^6.3.2",
        "core-js": "^3.27.1",
        "html-loader": "^1.3.2",
        "html-webpack-plugin": "^4.5.0",
        "mini-css-extract-plugin": "^1.6.2",
        "typescript": "^5.4.5",
        "webpack": "^5.79.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.13.2",
        "webpack-glsl-loader": "^1.0.1",
        "webpack-node-externals": "^3.0.0",
        "whatwg-fetch": "^3.5.0"
    }
}
